    android:textColor="#673AB7"
    android:textColorLink="#FFFFFF"
    android:textSize="24sp"
    app:cornerRadius="0dp"
    app:layout_constraintBottom_toTopOf="@+id/button12"
    app:layout_constraintEnd_toStartOf="@+id/button9"
